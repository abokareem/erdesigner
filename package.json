{
  "name": "erdesigner",
  "version": "1.0.0",
  "description": "tool to create beautiful er diagrams",
  "main": "./main.js",
  "scripts": {
    "dev": "NODE_ENV=development npm run build && npm run electron",
    "electron": "./node_modules/.bin/electron .",
    "clean:out": "rimraf out",
    "make:out": "./node_modules/.bin/mkdirp out",
    "build": "npm run clean:out && npm run make:out && npm run build:styles && npm run build:js",
    "build:js": "NODE_ENV=development rollup -c",
    "build:styles": "stylus src/browser/assets/main.styl -m -c -o out",
    "watch:js": "rollup -c",
    "watch:styles": "stylus src/browser/assets/main.styl -m -c -w -o out",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "precommit": "npm run lint",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Filip Vitas",
  "license": "MIT",
  "devDependencies": {
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.24.1",
    "electron": "^1.7.2",
    "electron-builder": "^18.2.2",
    "electron-packager": "^8.7.0",
    "eslint": "^3.19.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-config-standard-jsx": "^4.0.1",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-node": "^4.2.2",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^7.0.1",
    "eslint-plugin-standard": "^3.0.1",
    "husky": "^0.13.4",
    "jest": "^20.0.4",
    "onchange": "^3.2.1",
    "rimraf": "^2.6.1",
    "rollup": "^0.41.6",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-json": "^2.1.1",
    "stylus": "^0.54.5"
  },
  "dependencies": {
    "bluebird": "^3.5.0",
    "immutable": "^3.8.1",
    "preact": "^8.1.0"
  }
}
