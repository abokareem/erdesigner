{
  "name": "erdesigner",
  "version": "1.0.0",
  "description": "tool to create beautiful er diagrams",
  "main": "./main.js",
  "scripts": {
    "dev": "npm run build && npm run electron",
    "electron": "./node_modules/.bin/electron .",
    "clean:app": "rimraf app",
    "make:folder:assets": "./node_modules/.bin/mkdirp app/browser/assets",
    "copy:server": "cpx './src/**/*.{html,js}' app",
    "build": "npm run clean:app && npm run make:folder:assets && npm run build:styles && npm run build:js && npm run copy:server",
    "build:js": "rollup -c",
    "build:styles": "stylus src/browser/assets/main.styl -m -c -o app/browser/assets",
    "watch:js": "onchange 'src/**/*.js' -- npm run build:js",
    "watch:styles": "stylus src/browser/assets/main.styl -m -c -w -o app/browser/assets",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "precommit": "npm run lint",
    "postinstall": "install-app-deps",
    "electron:build": "build",
    "electron:build:win": "build --win",
    "electron:release": "build -p \"always\"",
    "release": "npm run build && npm run electron:release",
    "test": "echo \"Error: no test specified\""
  },
  "author": "Filip Vitas",
  "license": "MIT",
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.24.1",
    "cpx": "^1.5.0",
    "electron": "1.7.2",
    "electron-builder": "19.18.1",
    "eslint": "^3.19.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-config-standard-jsx": "^4.0.1",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-node": "^4.2.2",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^7.0.1",
    "eslint-plugin-standard": "^3.0.1",
    "husky": "^0.14.3",
    "jest": "^20.0.4",
    "onchange": "^3.2.1",
    "rimraf": "^2.6.1",
    "rollup": "^0.45.2",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-json": "^2.1.1",
    "stylus": "^0.54.5"
  },
  "dependencies": {
    "bluebird": "^3.5.0",
    "decko": "^1.2.0",
    "linkstate": "^1.0.1",
    "lodash": "^4.17.4",
    "preact": "^8.2.1",
    "preact-redux": "^2.0.2",
    "redux": "^3.7.2",
    "uuid": "^3.1.0"
  }
}
